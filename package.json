{
  "name": "mergeai",
  "displayName": "MergeAI",
  "version": "0.0.4",
  "description": "3-Panel Merge with working line updates",
  "engines": {
    "vscode": "^1.70.0"
  },
  "activationEvents": [
    "onCommand:mergeai.openCustomMergeView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mergeai.openCustomMergeView",
        "title": "Open Custom 3-Panel Merge View"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -watch -p ."
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "typescript": "^5.0.0"
  }
}
